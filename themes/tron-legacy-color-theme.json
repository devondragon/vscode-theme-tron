{
    "name": "Tron Legacy",
    "type": "dark",
    "colors": {
        "editor.background": "#0a0d12",
        "editor.foreground": "#7DFDFE",
        "activityBar.border": "#00F6FF",
        "activityBar.foreground": "#00F6FF",
        "activityBarBadge.background": "#00F6FF",
        "sideBarTitle.foreground": "#AED6F1",
        "titleBar.activeBackground": "#0c1115",
        "statusBar.background": "#0E131A",
        "statusBar.foreground": "#00F6FF",
        "statusBar.border": "#00F6FF",
        "terminal.background": "#0c1115",
        "terminal.foreground": "#7DFDFE"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#748994"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "string constant.other.placeholder",
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#B4F4FF"
            }
        },
        {
            "name": "Import Keywords",
            "scope": [
                "keyword.other.import.java"
            ],
            "settings": {
                "foreground": "#0B86B8"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#00FFFF"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#38FFFF"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted.double.json"
            ],
            "settings": {
                "foreground": "#97FFFF"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#00E5FF"
            }
        },
        {
            "name": "Class names",
            "scope": [
                "entity.name.type",
                "entity.name.class"
            ],
            "settings": {
                "foreground": "#00FFFF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Parameters",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#87CEEB"
            }
        }
    ]
}
